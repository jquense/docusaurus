{
  "name": "docusaurus-plugin-typedoc",
  "version": "1.0.0",
  "main": "lib/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/4Catalyzer/docusaurus-plugin-react-metadata.git"
  },
  "author": "4Catalyzer",
  "license": "MIT",
  "scripts": {
    "build": "4c build src",
    "format": "4c format src './*'",
    "lint": "4c lint src './*'",
    "prepublishOnly": "yarn run build",
    "release": "4c release",
    "tdd": "jest --watch",
    "test": "yarn lint && yarn typecheck && jest",
    "testonly": "jest",
    "typecheck": "tsc --noEmit"
  },
  "publishConfig": {
    "access": "public"
  },
  "prettier": "@4c/prettier-config",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*": [
      "yarn 4c lint --fix",
      "git add"
    ]
  },
  "jest": {
    "preset": "@4c/jest-preset",
    "testEnvironment": "node"
  },
  "private": true,
  "release": {
    "publishDir": "lib"
  },
  "devDependencies": {
    "@4c/babel-preset": "^8.0.3",
    "@4c/cli": "^2.0.1",
    "@4c/jest-preset": "^1.4.5",
    "@4c/prettier-config": "^1.1.0",
    "@4c/tsconfig": "^0.3.1",
    "@babel/preset-typescript": "^7.8.3",
    "@types/node": "*",
    "@typescript-eslint/eslint-plugin": "^4.14.1",
    "eslint-config-4catalyzer": "^1.1.0",
    "eslint-config-4catalyzer-jest": "^2.0.4",
    "eslint-config-4catalyzer-typescript": "^3.0.3",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jest": "^24.1.3",
    "eslint-plugin-prettier": "^3.1.2",
    "husky": "^4.3.8",
    "jest": "^26.0.0",
    "lint-staged": "^10.5.3",
    "prettier": "^2.0.1",
    "typescript": "^4.1.3"
  }
}
